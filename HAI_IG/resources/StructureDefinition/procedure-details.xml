    <element id="Procedure">
      <path value="Procedure" />
    </element>
    <element id="partOf1">
      <path value="Procedure.partOf" />
      <short value="partOf" />
      <definition
        value="MAY contain zero or one [0..1] Anesthesia Delivered (identifier: http://hl7.org/fhir/us/hai/StructureDefinition/anesthesia-delivered) (CONF:3347-185)." />
