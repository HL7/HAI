<StructureDefinition xmlns="http://hl7.org/fhir">
  <id
    value="hai-ssi-encounter" />
  <url
    value="http://hl7.org/fhir/us/hai/StructureDefinition/hai-ssi-encounter" />
  <name
    value="HAI SSI Encounter" />
  <status
    value="draft" />
  <publisher
    value="Lantana Consulting Group" />
  <contact>
    <name
      value="Sarah Gaunt" />
    <telecom>
      <system
        value="phone" />
      <value
        value="+6 141-029-2506" />
      <use
        value="work" />
    </telecom>
    <telecom>
      <system
        value="email" />
      <value
        value="sarah.gaunt@lantanagroup.com" />
      <use
        value="work" />
    </telecom>
  </contact>
  <description
    value="Key encounter data:&#xA;     The encounter type (inpatient or outpatient) is required.&#xA;     A value for admission date is required.&#xA;     A discharge date may be recorded.&#xA;" />
  <kind
    value="resource" />
  <abstract
    value="false" />
  <contextType
    value="resource" />
  <context
    value="Encounter" />
  <type
    value="Encounter" />
  <baseDefinition
    value="http://hl7.org/fhir/StructureDefinition/Encounter" />
  <derivation
    value="constraint" />
  <differential>
    <element
      id="Encounter">
      <path
        value="Encounter" />
    </element>
    <element
      id="status">
      <path
        value="Encounter.status" />
      <short
        value="status" />
      <definition
        value="SHALL contain exactly one [1..1] status=&quot;finished&quot; (CONF:3347-83)." />
      <min
        value="1" />
      <max
        value="1" />
      <type>
        <code
          value="code" />
      </type>
      <patternString
        value="finished" />
    </element>
    <element
      id="class">
      <path
        value="Encounter.class" />
      <short
        value="class" />
      <definition
        value="SHALL contain exactly one [1..1] class, which SHALL be selected from ValueSet NHSNEncounterTypeCode http://hl7.org/fhir/us/hai/ValueSet/nhsn-encounter-type-code (CONF:3347-84)." />
      <min
        value="1" />
      <max
        value="1" />
      <type>
        <code
          value="Coding" />
      </type>
      <binding>
        <strength
          value="required" />
        <valueSetReference>
          <reference
            value="http://hl7.org/fhir/us/hai/ValueSet/nhsn-encounter-type-code" />
          <display
            value="NHSNEncounterTypeCode" />
        </valueSetReference>
      </binding>
    </element>
    <element
      id="period">
      <path
        value="Encounter.period" />
      <short
        value="period" />
      <definition
        value="SHALL contain exactly one [1..1] period (CONF:3347-80)." />
      <min
        value="1" />
      <max
        value="1" />
    </element>
    <element
      id="period.start">
      <path
        value="Encounter.period.start" />
      <short
        value="start" />
      <definition
        value="This period SHALL contain exactly one [1..1] start (CONF:3347-81)." />
      <min
        value="1" />
      <max
        value="1" />
    </element>
    <element
      id="period.end">
      <path
        value="Encounter.period.end" />
      <short
        value="end" />
      <definition
        value="This period MAY contain zero or one [0..1] end (CONF:3347-82)." />
      <min
        value="0" />
      <max
        value="1" />
    </element>
    <element
      id="location">
      <path
        value="Encounter.location" />
      <short
        value="location" />
      <definition
        value="MAY contain zero or more [0..*] location (CONF:3347-196)." />
      <min
        value="0" />
      <max
        value="*" />
    </element>
    <element
      id="location.location">
      <path
        value="Encounter.location.location" />
      <short
        value="location" />
      <definition
        value="The location, if present, SHALL contain exactly one [1..1] HAI Healthcare Facility (identifier: http://hl7.org/fhir/us/hai/StructureDefinition/hai-healthcare-facility) (CONF:3347-197)." />
      <min
        value="1" />
      <max
        value="1" />
      <type>
        <code
          value="Reference" />
        <targetProfile
          value="http://hl7.org/fhir/us/hai/StructureDefinition/hai-healthcare-facility" />
      </type>
    </element>
  </differential>
</StructureDefinition>